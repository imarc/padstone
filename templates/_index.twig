{% import "/_macros" as macros %}
{% set section = 'home' %}
{% extends "_layouts/main" %}
{% block content %}
    <!-- Main jumbotron for a primary marketing message or call to action -->
    <div class="jumbotron">
        <div class="container">
            <h1 class="display-3">{{ entry.headline }}</h1>
            <p class="lead">{{ entry.subheadline }}</p>
            <pre>
                {{ entry ? entry.headline|lower : '' }}
                {{ entry ? entry|lower }}
                {{ (entry ?: '')|lower }}
            </pre>
        </div>
    </div>

    {{ macros.renderAllBlocks(entry.designedContent, true) }}

{% endblock %}
